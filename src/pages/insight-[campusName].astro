        <clipPath id="clip0_1125_7345">
          <rect width="20.6897" height="20.6896" fill="white" transform="matrix(0 1 -1 0 20.6895 0.289062)"/>
        </clipPath>
      </defs>
    </svg>
    <Text type="subtitle" class='text-black-100 text-[16px] text-center'>
      EXPLORE THE activities OFFERED
    </Text>
    <svg xmlns="http://www.w3.org/2000/svg" width="21" height="21" viewBox="0 0 21 21" fill="none">
      <g clip-path="url(#clip0_1125_7345)">
        <path fill-rule="evenodd" clip-rule="evenodd" d="M0.689772 10.6337C0.689772 15.9661 5.01251 20.2889 10.3449 20.2889C15.6773 20.2889 20 15.9661 20 10.6337C20 5.3013 15.6773 0.978516 10.3449 0.978516C5.01251 0.978516 0.689772 5.3013 0.689772 10.6337ZM13.7535 11.0872C14.0363 11.37 14.0363 11.8285 13.7535 12.1113L10.8569 15.0078C10.5741 15.2906 10.1156 15.2906 9.83285 15.0078L6.93631 12.1113C6.65352 11.8285 6.65352 11.37 6.93631 11.0872C7.2191 10.8044 7.6776 10.8044 7.96039 11.0872L9.62075 12.7476V6.77164C9.62075 6.37171 9.94496 6.0475 10.3449 6.0475C10.7448 6.0475 11.069 6.37171 11.069 6.77164V12.7476L12.7294 11.0872C13.0122 10.8044 13.4707 10.8044 13.7535 11.0872Z" fill="#564101"/>
      </g>
      <defs>
        <clipPath id="clip0_1125_7345">
          <rect width="20.6897" height="20.6896" fill="white" transform="matrix(0 1 -1 0 20.6895 0.289062)"/>
        </clipPath>
      </defs>
    </svg>
  </div>
  <div class="flex items-center justify-center gap-r overflow-scroll max-sm:flex-col">
    {
      facilities?.map(item => {
          return  <div class={`flex flex-col max-w-[400px] w-full border-t-0 flex-shrink-0`}
                       
                  >
                      <Image src={item.images[0]} className='border-b-[5px] border-yellow h-[450px] object-cover max-sm:h-[250px]' />
                      <div class="p-l flex flex-col gap-r  border-[1px] border-t-0 border-gray-900">
                        <Text type="h4" class="text-neutral-dark-grey font-display font-bold">
                          {item.title}
                        </Text>
                        <Text type="p" class='text-neutral-dark-grey'>
                          {item.description}
                        </Text>
                      </div>
                  </div>
      })
    }
  </div>
 
</div>
<!-- section  -->
<div id="achievements" class="p-xl flex flex-col justify-center gap-l items-center scroll-m-14 max-sm:p-0">
  <div class="px-r flex flex-col gap-l max-sm:gap-r">
    <Heading first="Celebrating" second={"Achievements"} subtitle="INSPIRING FEATS">
    </Heading>
    <Text type="p" class="text-center">We take immense pride in celebrating academic triumphs that reflect the studentsâ€™ dedication.</Text>
  </div>

    <div class="flex justify-center gap-[2px] overflow-scroll w-full max-sm:flex-col  max-sm:items-center max-sm:gap-[16px] ">
      {toppers?.map((item,index) => {
        return <div class="max-w-[287px] h-[525px] w-full flex flex-col justify-end  topper-card relative overflow-hidden flex-shrink-0
                          "
        >
          <div 
            class="absolute inset-0 -z-[1] saturate-0 topper-img"
            style={{
              "--topper-img": `url(${item.image})`
            }}
           
      
          ></div>
          <div class="flex flex-col items-center gap-[7px] mb-10">
        
            <Text type='p' class='text-yellow font-display font-bold'>
              {item.ranks}
            </Text>
              <Text type='h4' class='text-center text-white font-display font-bold' set:html={item.name}>
               
              </Text>
            <Text type='h4' class='text-yellow font-display font-bold' >
              97.8%
            </Text>
          </div>
